train_net: "finetune_salicon_res.prototxt"
base_lr: 0.0001
lr_policy: "step"
#lr_policy: "step"
gamma: 0.9
stepsize: 10000
display: 20
#average_loss: 100
iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "finetuned_salicon"
#debug_info: true
solver_mode: GPU
type: "SGD"

